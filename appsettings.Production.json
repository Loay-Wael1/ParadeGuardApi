{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ApiSettings": {
    "NasaBaseUrl": "https://power.larc.nasa.gov/api/temporal/daily/point",
    "OpenCageBaseUrl": "https://api.opencagedata.com/geocode/v1/json",
    "RequestTimeoutSeconds": 45,
    "MaxRetryAttempts": 3,
    "RateLimitPerMinute": 60,
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://raha-lcast-nasa-project.vercel.app",
      "http://localhost:5173",
      "https://paradeguardapi-production.up.railway.app"
    ]
  },
  "Cache": {
    "DefaultExpirationMinutes": 720,
    "GeocodingExpirationMinutes": 1440,
    "WeatherExpirationMinutes": 720,
    "MaxCacheItems": 500
  },
  "AllowedHosts": "*"
}